	<fonctions>creer_sprites_fonctions.php</fonctions>
	<pipeline>
		<nom>recuperer_fond</nom>
	</pipeline>
	<necessite id="SPIP" version="[2.0.0;2.1.99]" />
	<categorie>multimedia</categorie>
</plugin>


