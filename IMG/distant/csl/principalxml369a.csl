	</pipeline>
	<options>bibles_options.php</options>
	<fonctions>bible_fonctions.php</fonctions>
	
	<categorie>divers</categorie>
	<lien>http://www.spip-contrib.net/Spip-Bible,1125</lien>
 
</plugin>


