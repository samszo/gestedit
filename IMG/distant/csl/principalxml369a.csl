    <etat>dev</etat>
    <prefix>hydraulic</prefix>
    <fonctions>hydraulic_fonctions.php</fonctions>
    <pipeline>
        <nom>insert_head</nom>
        <inclure>hydraulic_pipelines.php</inclure>
    </pipeline>
</plugin>


